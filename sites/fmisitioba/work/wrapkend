    window.$(window).resize(initialize);
    window.addEventListener("orientationchange", initialize, false);


    function isHomeSection() {
        var route = window.location.href;
        route = route.replace(window.location.origin, '').replace(/\//g, "");
        return route === '';
    }
    
    function isAboutSection(){
        return window.location.href.indexOf('about-misitioba') !== -1;
    }

    function isMobile() {
        return window.innerWidth < 992;
    }

    function shouldConfigureBackgroundVideo() {
        return (isHomeSection()||isAboutSection()) && !isMobile();
    }







    function initialize() {
        if (window.initializing) return;
        window.initializing = true;
        console.log('initialize');
        if (isMobile()) {
            startMobile();
        }
        else {
            
            configureDesktop();
        }
        window.initializing = false;
    }

    function loadBackgroundMovie() {
        window.$.getScript('/vendor/jquery.vide.min.js', function() {
            let videoUrl = '/files/video.mp4';
            //var req = new XMLHttpRequest();
            // req.open('GET', videoUrl, true);
            // req.onload = function() {
            // if (this.status === 200) {
            let $video = window.$('#wrap');
            $video.vide({
                mp4: videoUrl,
                poster: '/files/poster.jpg'
            }, {
                posterType: 'jpg',
                loop: true,
                muted: false,
                position: '0% 0%'
            });
            //   }
            // };
            // req.send();
        });
    }

    function getBackgroundMovieInstance() {
        return window.$('#wrap').data('vide');
    }

    function removeBackgroundMovie() {
        var m = getBackgroundMovieInstance();
        if (m) m.destroy();
    }

    function configureDesktop() {
        console.log('configureDesktop');
        if (shouldConfigureBackgroundVideo()) {
            var movie = getBackgroundMovieInstance();
            if (movie) {
                movie.resize();
            }
            else {
                console.log('loadBackgroundMovie');
                loadBackgroundMovie();
            }
            window.$('.content-wrapper').css('background','transparent');
        }else{
            removeBackgroundMovie();
            window.$('html').css('background-image', 'url(/img/slider/1.jpg)');
            window.$('.content-wrapper').css('background','transparent');
        }
    }

    function startMobile() {
        removeBackgroundMovie();
        window.$('html').css('background-image', '');
    }




    function startSlider() {
        var sliderMax = 5;
        var sliderCur = 1;
        var arr = [];
        for (var x = 1; x <= sliderMax; x++) {
            arr.push('/img/slider/' + x + '.jpg');
        }
        var s = Date.now();
        if (window.$('.slider').length <= 0) {
            console.warn('slider images are not available');
            return;
        }
        var texts = ['Digital tools', 'Freelance services', 'Mentoring', "Couching"];
        var cycle = function() {
            if (sliderCur > sliderMax) sliderCur = 1;
            if (window.innerWidth < 992 && sliderCur === 1) {
                sliderCur = 2;
            }
            console.log('Sliding ', sliderCur);
            window.$('.slider').css('background-image', 'url(' + '/img/slider/' + sliderCur + '.jpg)');
            window.$('.slider-text-block h2').html(texts[Math.round(Math.random() * (texts.length - 1 - 0) + 0)]);
            sliderCur++;
        };
        loadImages(arr, function() {
            console.log('Images OK', ((Date.now() - s) / 1000).toFixed(2));
            setInterval(function() {
                cycle();
            }, 5000);
        });
        cycle();
    }



    function loadImages(images, callback) {
        var total = images.length,
            count = 0,
            i;

        function check(n) {
            if (n == total) {
                callback();
            }
        }
        for (i = 0; i < total; ++i) {
            var src = images[i];
            var img = document.createElement("img");
            img.src = src;

            img.addEventListener("load", function() {
                if (this.complete) {
                    count++;
                    check(count);
                }

            });
        }
    }</script><script src=/js/bundle.js></script><div id=fb-root></div><script>(function(d, s, id) {
