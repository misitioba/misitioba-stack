extends incuLayout.pug
block content
	input#password.form-control(type="password" style="max-width:200px;")
	button.btn.btn-primary.mt-2(href="/admin/articles" onclick="login(event)") Login
	script.
		function login(e){
			if($('#password').val()==='gtf'){
				window.localStorage.setItem('auth','admin');
				window.location.href="/admin"
			}
		}